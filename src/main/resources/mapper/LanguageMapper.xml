<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.carbarn.inter.mapper.LanguageMapper">

    <select id="getLanguage" resultType="com.carbarn.inter.pojo.language.LanguageDTO">
        SELECT
        language_name,
        language
        FROM
        `language`
        where
        enable_flag = 0
        order by id
    </select>
</mapper>